{
  "routes": [
    {
      "src": "^/_perps/_next/(.*)$",
      "dest": "https://ftx2-perps.vercel.app/_next/$1"
    },
    {
      "src": "^/_perps/(.*)$",
      "dest": "https://ftx2-perps.vercel.app/$1"
    },

    {
      "src": "^/perps/?$",
      "dest": "/perps.html"
    },
    {
      "src": "^/perps\\.html$",
      "status": 308,
      "headers": { "Location": "/perps" }
    },

    {
      "handle": "filesystem"
    }
  ]
}

