{
  "rewrites": [
    {
      "source": "/perps",
      "destination": "https://ftx2-perps.vercel.app"
    },
    {
      "source": "/perps/:path*",
      "destination": "https://ftx2-perps.vercel.app/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://ftx2-perps.vercel.app/_next/:path*"
    }
  ]
}

